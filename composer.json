{
    "name": "atxy2k/happypay",
    "description": ":package_description",
    "license": "license",
    "authors": [
        {
            "name": "Ivan alvarado",
            "email": "ivan.alvarado@serprogramador.es",
            "homepage": "https://serprogramador.es"
        }
    ],
    "homepage": "https://github.com/atxy2k/happypay",
    "keywords": ["Laravel", "HappyPay"],
    "require": {
        "illuminate/support": "~6|~7",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Atxy2k\\HappyPay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Atxy2k\\HappyPay\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Atxy2k\\HappyPay\\HappyPayServiceProvider"
            ],
            "aliases": {
                "HappyPay": "Atxy2k\\HappyPay\\Facades\\HappyPay"
            }
        }
    }
}
